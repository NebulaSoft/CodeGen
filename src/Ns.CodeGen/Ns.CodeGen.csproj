<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <LangVersion>11</LangVersion>
        <TargetFrameworks>net6.0;net7.0</TargetFrameworks>
        <Title>Code Generation/Compiler Helper</Title>
        <Authors>NebulaSoft</Authors>
        <Copyright> Copyright Â© 2017 NebulaSoft Ltd</Copyright>
        <PackageProjectUrl>https://github.com/NebulaSoft/CodeGen</PackageProjectUrl>
        <PackageLicenseUrl>https://github.com/NebulaSoft/CodeGen</PackageLicenseUrl>
        <RepositoryUrl>https://github.com/NebulaSoft/CodeGen</RepositoryUrl>
        <RepositoryType>Git</RepositoryType>
        <PackageTags>C#,Compiler,Code,Generation</PackageTags>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <PackageId>NebulaSoft.CodeGen</PackageId>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="ExpressionToCodeLib" Version="3.5.0" />
      <PackageReference Include="Microsoft.CodeAnalysis.CSharp" Version="4.6.0" />
    </ItemGroup>

</Project>
